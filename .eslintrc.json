{
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "prettier"
    ],
    "rules": {
        "prettier/prettier": "error",
        "no-console": "off",
        "no-alert": "off",
        "no-unused-vars": "warn",
        "no-param-reassign": "off",
        "no-nested-ternary": "off",
        "no-plusplus": "off",
        "import/extensions": "off",
        "no-restricted-globals": "off"
    }
}